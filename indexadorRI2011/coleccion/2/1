                jQuery(function() {
                    $('#details1 .descriptions-tab-thumbnail').click(function(event) {
                            tab2trigger($('li.playlist-menu-1'));
                     });
                    $('.gotop').click(function(){
                        //window.scrollTo(0, 0);
                        $('html').animate({scrollTo: ('.playlist-page')}, 'slow');//IE, FF
                        $('body').animate({scrollTo: ('.playlist-page')}, 'slow');//chrome
                    })
                    // ordering logic
                    var pl_len = $('#details1 .video-desc').length;
                    var init_vlist_value = false;
                    var new_vlist_value = false;
                    $('#details1 .details-manual-order').hover(function(e) {
                        $(this).select();
                        if (!init_vlist_value) { init_vlist_value = parseInt($(this).val()); }
                    });
                        $('#details1 .details-manual-order').focus(function(e) {
                            if (!init_vlist_value) { init_vlist_value = parseInt($(this).val()); }
                        });
                        //$('#details1 .details-manual-order').blur(function(e) {
                        //  $(this).parent().find('.details-manual-order-confirm').hide();
                        //})
                        $('#details1 .details-manual-order').keyup(function(e) {
                            $(this).parent().find('.details-manual-order-confirm').hide();
                            new_vlist_value = parseInt($(this).val());
                            if (isNaN(new_vlist_value)) { return false; }
                            if (new_vlist_value == undefined) { return false; }
                            if (new_vlist_value == parseInt(init_vlist_value)) { return false; }
                            if (new_vlist_value < 0) { return false; }
                            if (new_vlist_value > pl_len) { return false; }
                            if (new_vlist_value == 0) { new_vlist_value = 1; }
                            $(this).parent().find('.details-manual-order-confirm').show();
                        });
                        // add the keypress event
                        $('#details1 .details-manual-order').live('keypress', function(e) {
                            if(e.keyCode == 13) {
                                tmp = $('#details1 .video-desc').eq(init_vlist_value-1);
                                target = $('#details1 .video-desc').eq(new_vlist_value-1);
                                tmpc = tmp.clone(true);
    
                                target.find('.details-manual-order').val(init_vlist_value);
                                tmp.replaceWith(target);
                                place = $('#details1 .video-desc').eq(new_vlist_value-1);
                                tmpc.insertBefore(place);
    
                                var items = $('#details1 .video-desc .details-manual-order');
                                for (var i = 0; i < items.length; i++) {
                                    $(items[i]).val(i+1);
                                }

                                videoplayers[1].switchPlaylistItems(init_vlist_value, new_vlist_value);
                                init_vlist_value = false;
                                new_vlist_value = false;
                                $('#details1 .details-manual-order-confirm').hide();
                            }
                        });

                        $('#details1-items .details-manual-order-confirm').live('click', function(e) {
                                tmp = $('#details1 .video-desc').eq(init_vlist_value-1);
                                target = $('#details1 .video-desc').eq(new_vlist_value-1);
                                tmpc = tmp.clone(true);
    
                                target.find('.details-manual-order').val(init_vlist_value);
                                tmp.replaceWith(target);
                                place = $('#details1 .video-desc').eq(new_vlist_value-1);
                                tmpc.insertBefore(place);
    
                                var items = $('#details1 .video-desc .details-manual-order');
                                for (var i = 0; i < items.length; i++) {
                                    $(items[i]).val(i+1);
                                }

                                videoplayers[1].switchPlaylistItems(init_vlist_value, new_vlist_value);
                                init_vlist_value = false;
                                new_vlist_value = false;
                                $('#details1 .details-manual-order-confirm').hide();
                        });
                });
